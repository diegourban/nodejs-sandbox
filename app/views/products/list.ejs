<html>
    <body>
        <table>
            <tr>
                <td>ID</td>
                <td>Title</td>
                <td>Description</td>
                <td>Price</td>
            </tr>
            <%for(var i = 0; i < lista.length; i++) {%>
                <tr>
                    <td><%=lista[i].id%></td>
                    <td><%=lista[i].title%></td>
                    <td><%=lista[i].description%></td>
                    <td><%=lista[i].price%></td>
                </tr>
            <%}%>
        </table>
    </body>
</html>
